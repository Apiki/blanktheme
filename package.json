{
  "name": "blanktheme",
  "version": "1.0.0",
  "description": "Theme Boilerplate",
  "repository": "https://github.com/Apiki/blanktheme.git",
  "author": "Apiki",
  "license": "GPL-2.0",
  "scripts": {
    "storybook": "start-storybook -p 9001 -c storybook -s ./assets/images",
    "phpcs": "phpcs --colors -p ./",
    "phpcs:diff": "git diff --name-only --cached --diff-filter=ACMRTUXB -- . ':(exclude)vendor/*' | xargs phpcs --colors -p",
    "precommit": "yarn phpcs:diff",
    "prepush": "yarn phpcs"
  },
  "devDependencies": {
    "@storybook/addon-options": "^3.2.12",
    "@storybook/react": "^3.2.12",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.4",
    "husky": "^0.14.3",
    "import-glob-loader": "^1.1.0",
    "node-sass": "^4.5.3",
    "react-element-to-jsx-string": "^12.0.0",
    "react-highlight": "^0.10.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0"
  },
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
