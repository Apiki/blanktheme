{
  "name": "apiki/blanktheme",
  "description": "A Apiki blanktheme.",
  "type": "wordpress-theme",
  "license": "GPL-2.0",
  "require": {
    "php": ">=5.3",
    "composer/installers": "^1.0",
    "tgmpa/tgm-plugin-activation": "^2.6",
    "aristath/kirki": "^3.0"
  },
  "extra": {
    "installer-paths": {
      "vendor/{$name}/": ["type:wordpress-plugin"]
    }
  },
  "autoload": {
    "psr-4": {
      "Blanktheme\\": "src/"
    },
    "files": [
      "vendor/kirki/kirki.php"
    ]
  }
}
